package com.example.planAndGoCRUD.repository;

import com.example.planAndGoCRUD.entity.UserBirthday;
import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

@Repository
public interface UserBirthdayRepository extends JpaRepository<UserBirthday, Long> {
    // 생일까지 남은 날짜 기준으로 조회 같은 기능은 나중에 추가 가능
}
